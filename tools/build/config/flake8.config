[flake8]
max-line-length = 120
max-complexity = 10
filename = *.py
format = default
exclude = 
  .git,
  __pycache__,
  out/,
  # exclude the protobuf generated code
  *_pb2.py,
  # this is temporary
  ../../../test/robot,
  # paths are relative to the location of the configuration file
  ../../../src/backend/lambda_functions/*/packages
