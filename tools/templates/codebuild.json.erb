{
  "projectName": "<%= project_name %>",
  "sourceVersion": "<%= source_version %>",
  "environmentVariablesOverride": [
    {
      "name": "TARGET_ENVIRONMENT",
      "value": "<%= target_environment %>",
      "type": "PLAINTEXT"
    },
    {
      "name": "ACCOUNT",
      "value": "<%= account %>",
      "type": "PLAINTEXT"
    }<%if !release_version.empty?%>,
    {
      "name": "RELEASE_VERSION",
      "value": "<%= release_version %>",
      "type": "PLAINTEXT"
    }<% end %>
  ]
}